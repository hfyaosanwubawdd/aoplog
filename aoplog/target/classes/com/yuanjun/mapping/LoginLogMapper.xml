<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yuanjun.dao.LoginLogDao" >
  
  <insert id="saveLoginLog" parameterType="com.yuanjun.bean.LoginLog">
  	insert into loginlog(loginName,loginIp,loginTime,loginOutTime,loginStatus)
  	values(#{loginName},#{loginIp},#{loginTime},#{loginOutTime},#{loginStatus})
  </insert>
  
</mapper>